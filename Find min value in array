#include <stdio.h>
#include <stdlib.h>


int main()
{
    int min,i,tab[10];

printf("Enter 10 integers: ");
  for(i=0;i<10;i++){
      scanf("%d \t",&tab[i]);
     }
min=tab[0];
  for(i=0;i<10;i++){
    if(min>tab[i]){
        min=tab[i];
       }
    }

printf("the minimum value of the inputs is %d",min);
return 0;

}
