#include <stdio.h>
#include <stdlib.h>


int main()
{
int t[100],i,n;

printf("enter the size of the array :\n");
scanf("%d",&n);
printf("enter the %d elements of the array:\n",n);

for(i=0;i<n;i++){
    scanf("%d",&t[i]);
}
printf("the reversed array is \n");
for(i=n-1;i>=0;i--){
    printf("%d \n",t[i]);
}

return 0;

}
